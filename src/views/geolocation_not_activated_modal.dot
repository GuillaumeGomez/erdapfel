<div class="modal__maps">
  <i class="icon-x modal__close" {{= click(this.close, this) }}></i>
  <h2 class="modal__title">{{= _('Geolocation not available.', 'geolocation') }}</h2>
  <span class="modal__subtitle">{{= _('Qwant Maps cannot access your position. Did you activate the GPS?', 'geolocation') }}</span>
  <button class="modal__button" {{= click(this.close, this) }}>{{= _('OK', 'geolocation') }}</button>
</div>
