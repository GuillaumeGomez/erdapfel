{{? this.isMasqEnabled}}
  <div class="menu__panel__top__masq">
    <div class="menu__panel__masq_icon_container">
      <div class="icon-masq menu__panel__masq_icon" {{= click(this.openMasq, this)}}></div>
    </div>
    {{? this.isLoggedIn}}
      <div class="menu__panel__top__masq__login__button menu__panel__top__masq__login__button_logged" {{= click(this.openMasq, this)}}>

        {{? this.profileImage }}
          <img src={{= this.profileImage }} class="menu__panel__top__masq_profile_icon">
        {{??}}
          <div class="menu__panel__top__masq_profile_icon menu__panel__top__masq_profile_icon_default {{= this.defaultProfileImage.backgroundColor}}">
            <div>{{= this.defaultProfileImage.letter}}</div>
          </div>
        {{?}}
        <span>{{= this.username }}</span>

      </div>
      <div class="menu__panel__top__masq__second__button_container">
          <div class="icon-log-out"></div>
          <div class="menu__panel__top__masq__second__button" {{= click(this.logout, this)}}>{{= _('Deactivate') }}</a>
      </div>
    {{??}}
      <input
        type="button"
        class="menu__panel__top__masq__login__button"
        value="{{= _('Activate') }}"
        {{= click(this.login, this) }}
      >
      <div class="menu__panel__top__masq__second__button_container">
          <div class="icon-discover"></div>
          <div class="menu__panel__top__masq__second__button" {{= click(this.openMasqOnboarding, this)}}>{{= _('How does it work ?') }}</a>
      </div>
    {{?}}
  </div>
{{?}}
