<div class="service_panel{{= this.active ? ' service_panel--active' : ''}}">

  <h3 class="service_panel__title">
    {{= _('Qwant Maps services', 'service panel') }}
  </h3>

  <div {{= click(this.toggleFavorite, this) }} class="service_panel__item service_panel__item__fav{{= this.isFavoriteActive ? ' side_bar__item--active' : ''}}">
    <button class="service_panel__item__action">
      <i class="icon-icon_star side_bar__item__icon"></i>
      <span class="service_panel__item__text">{{= _('YOUR FAVORITES', 'service panel') }}</span>
     </button>
  </div>

  <div {{= click(this.toggleDirection, this) }} class="service_panel__item service_panel__item__direction{{= this.isDirectionActive ? '' : ' service_panel__item--disabled'}}" >
    <button class="service_panel__item__action">
      <i class="icon-corner-up-right side_bar__item__icon"></i>
      <span class="service_panel__item__text">{{= _('DIRECTIONS', 'service panel') }}</span>
    </button>
  </div>

  <div class="service_panel__categories">

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #495063">
        <span class="icon icon-lodging"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Hotel") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #ef8000">
        <span class="icon icon-restaurant"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Restaurant") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #e93865">
        <span class="icon icon-cinema"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Cinema") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #ef8000">
        <span class="icon icon-bar"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Bar") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #f3c600">
        <span class="icon icon-grocery"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Supermarket") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #495063">
        <span class="icon icon-bank"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Bank") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #4aa0e7">
        <span class="icon icon-pharmacy"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Pharmacy") }}</div>
    </div>

    <div class="service_panel__category">
      <div class="service_panel__category__icon" style="background: #495063">
        <span class="icon icon-school"></span>
      </div>
      <div class="service_panel__category__title">{{= _("Education") }}</div>
    </div>





  </div>
</div>
