<div class="poi_panel__info__wiki">
  {{? this.wiki.description }}
    <p class="poi_panel__description__ellipsis">
    {{= this.htmlEncode(this.wiki.description) }}
    </p>
  {{?}}
  <br>
  {{? this.wiki.url }}
    <a class="poi_panel__info__wiki__link" rel="noopener noreferrer" target="_blank" href="{{= this.htmlEncode(this.wiki.url) }}">
      <i class="icon-chevrons-right"></i><span>{{= _('WIKIPEDIA') }}</span></a>
  {{?}}

  {{? !this.wiki.description && !this.wiki.url }}
    <a class="poi_panel__info__wiki__link" rel="noopener noreferrer" target="_blank" href="https://www.wikidata.org/wiki/{{= this.htmlEncode(this.tag) }}">
      <i class="icon-chevrons-right"></i><span>{{= _('WIKIDATA') }}</span></a>
  {{?}}
</div>
